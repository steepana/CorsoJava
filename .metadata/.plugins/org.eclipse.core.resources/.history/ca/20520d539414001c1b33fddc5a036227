package org.corso;
public class Main {

	public static void main(String[] args) {
		
		Animali gatto= new Animali();
		Animali cane= new Animali();
	
	
		gatto.setNome("micio");
		gatto.setColore("nero");
		gatto.setTipo("siamese");
		
		
	
		cane.setNome("fido");
		cane.setColore("marrone");
		cane.setTipo("pastore tedesco");
		
	System.out.println(gatto.getNome() + "  "+ gatto.getColore() + "  " + gatto.getTipo());
	System.out.println(cane.getNome() + "  "+ cane.getColore() + "  " + cane.getTipo());

	
	
	
	}

}
