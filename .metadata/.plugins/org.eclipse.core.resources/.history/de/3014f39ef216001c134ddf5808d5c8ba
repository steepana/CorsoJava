package org.saluti.corso;

public class ciao {

	public static void main(String[] args) {
		
		Saluto francese = new Saluto();
		francese.setNazione("francia");
		francese.getModoSaluto("bonjour");
		
		
		Saluto inglese = new Saluto();
		inglese.setInghilterra("Goodmorning");
		
		
		Saluto italiano = new Saluto();
		italiano.setItalia("Goodmorning");
		

	}

}
